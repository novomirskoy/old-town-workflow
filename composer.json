{
  "name": "old-town/workflow",
  "description": "Workflow Module",
  "type": "library",
  "license": "BSD-3-Clause",
  "keywords": [
    "workflow",
    "library"
  ],
  "minimum-stability": "dev",
  "homepage": "https://github.com/old-town/old-town-workflow",
  "support": {
    "email": "and-rey2@yandex.ru"
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "git@github.com:old-town/old-town-propertyset.git"
    },
    {
      "type": "vcs",
      "url": "git@github.com:old-town/old-town-log.git"
    },
    {
      "type": "vcs",
      "url": "git@github.com:onema/php-coveralls.git"
    }
  ],
  "require": {
    "php": ">=5.5",
    "old-town/propertyset": "dev-dev",
    "old-town/log": "dev-dev",
    "zendframework/zend-diactoros": "~1.3",
    "zendframework/zend-stdlib": "~2.4",
    "psr/http-message": "~1",
    "psr/log": "~1"

  },
  "require-dev": {
    "ext-mbstring": "*",
    "fabpot/php-cs-fixer": "~1.11",
    "phpunit/phpunit": "~4.8",
    "phpunit/phpcov": "~2",
    "zendframework/zend-loader": "~2.4",
    "satooshi/php-coveralls": "~1",
    "behat/behat": "~3.0",
    "vipsoft/code-coverage-extension": "~3@dev",
    "internations/http-mock": "dev-master",
    "ramsey/uuid" : "~3.0.0"
